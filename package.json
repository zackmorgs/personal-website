{
  "name": "zackmorgs.github.io",
  "version": "0.1.0",
  "private": true,
  "homepage": "",
  "dependencies": {
    "git-state": "^4.0.0",
    "node-sass-chokidar": "^1.3.0",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4",
    "readline": "^1.3.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive --output-style compressed --source-map-embed",
    "help-sass": "node-sass-chokidar --help",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build"
  },
  "scripts-for-later": {
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "eslint-plugin-react": "^7.9.1",
    "gh-pages": "^1.2.0",
    "semistandard": "6.1.0"
  }
}
