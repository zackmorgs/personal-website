{
	"name": "personal-website",
	"version": "0.1.0",
	"private": true,
	"homepage": "http://localhost:3000/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zackmorgs/personal-website"
	},
	"dependencies": {
		"babel-plugin-macros": "^2.2.2",
		"boilerplate.css": "^1.0.8",
		"hamburgers": "^0.9.3",
		"lint-staged": "^7.2.0",
		"lqip.macro": "^0.1.5",
		"node-sass-chokidar": "^1.3.0",
		"normalize.css": "^8.0.0",
		"npm-run-all": "^4.1.3",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-feather": "^1.1.0",
		"react-icons": "^2.2.7",
		"react-ideal-image": "0.0.5",
		"react-loading": "^2.0.2",
		"react-scripts": "1.1.4",
		"react-transition-group": "^2.3.1",
		"react-waypoint": "^8.0.2",
		"readline": "^1.3.0",
		"require-from-string": "^1.2.1",
		"reset.css": "^2.0.2",
		"sanitize.css": "^5.0.0"
	},
	"browserslist": [
		"Chrome >= 45",
		"Firefox >= 38",
		"Edge >= 12",
		"Explorer >= 10",
		"iOS >= 9",
		"Safari >= 9",
		"Android >= 4.4",
		"Opera >= 30"
	],
	"scripts": {
		"start": "npm-run-all -p watch-css start-js",
		"start-js": "react-scripts start",
		"build": "npm-run-all build-css build-js",
		"build-js": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"build-css": "node-sass-chokidar ./src/Styles/Pages/index/scss/index.scss ./src/Styles/Pages/index/css/index.css",
		"watch-css-old": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive --source-map-embed",
		"watch-css": "npm run build-css && node-sass-chokidar -w ./src/Styles/Pages/index/scss/ -o ./src/Styles/Pages/index/css/",
		"help-sass": "node-sass-chokidar --help",
		"predeploy": "npm run build",
		"deploy": "gh-pages -b master -d build",
		"eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
	},
	"scripts-for-later": {
		"eject": "react-scripts eject"
	},
	"devDependencies": {
		"@types/react": "^16.4.0",
		"@types/react-dom": "^16.0.6",
		"babel-cli": "^6.26.0",
		"babel-preset-react": "^6.24.1",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-react": "^7.9.1",
		"gh-pages": "^1.2.0",
		"husky": "^0.14.3",
		"prettier": "^1.13.5",
		"pretty-quick": "^1.6.0",
		"react-ideal-image": "0.0.5",
		"semistandard": "^6.1.2"
	}
}
